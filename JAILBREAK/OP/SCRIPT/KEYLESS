repeat wait() until game:IsLoaded() and game.Players.LocalPlayer

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/3xjn/UI-Library/main/UI"))()
local Window = Library:CreateWindow("OP Jailbreak Script")

local AutoTab = Window:CreateTab("Auto Rob")
local TeleportTab = Window:CreateTab("Teleport")
local CombatTab = Window:CreateTab("Combat")
local VehicleTab = Window:CreateTab("Vehicle")
local MiscTab = Window:CreateTab("Miscellaneous")

AutoTab:CreateToggle("Auto Rob Jewelry", false, function(value)
    getgenv().AutoRobJewelry = value
    while getgenv().AutoRobJewelry do
        -- Auto Rob Jewelry Code Here
        wait()
    end
end)

